<odoo>
    <data noupdate="1">
        <!-- Sequence for Farmers -->
        <record id="seq_farmers_code" model="ir.sequence">
            <field name="name">Farmers Code Sequence</field>
            <field name="code">ilo.employee.farmers.sequence</field>
            <field name="padding">3</field>  <!-- Generates FA001, FA002, etc. -->
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

